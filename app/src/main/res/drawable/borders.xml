<?xml version="1.0" encoding="utf-8"?>
<!-- This shape is used to draw a border around account information on the account info screen -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item><shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
        <stroke android:width="3dp" android:color="@color/account_border" />
        <corners android:radius="10px"/>
    </shape></item>
</selector>